function log(logText: Text, file: Text):
  log "%{_logText}%" to "%{_file}%"

on command "mute":
	if player has permission "group.vagt", "group.mod", "group.admin" or "*":
		log("%player% - %command% %arguments% - TID: %hour%:%minute%:%second%", "mute.log")

